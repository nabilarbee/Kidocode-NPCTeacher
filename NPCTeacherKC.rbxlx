local function writeText(text, waitTime)
	TriggerWaitQuestion:FireServer()
	local userClicked = false
	local connection

	-- Removes player's ability to move
	sound.Volume = 5
	humanoid.WalkSpeed = 0
	humanoid.JumpPower = 0
	btnNext.Visible = true
	btnNextBox.Visible = true
	gui.Enabled = true
	textBox.Visible = true

	-- Detect for click on the Next Button
	-- Need to configure GUI to be able to select multiple options
	connection = btnNext.MouseButton1Down:Connect(function()
		userClicked = true
		clickSound:Play()
	end)

	-- Looping EACH character in string
	for i = 1, #text do
		if userClicked then
			textLabel.Text = text -- skip to full text
			break
		end
		textLabel.Text = string.sub(text, 1, i)
		task.wait(0.035)
		sound:Play()
	end

	sound:Stop()

	-- Waits for the amount of waitTime given
	if waitTime ~= 0 then
		local elapsed = 0
		while elapsed < waitTime and not userClicked do
			elapsed += task.wait()
		end
	else
		while not userClicked do
			task.wait()
		end
	end

	-- Disconnect connection
	if connection then connection:Disconnect() end
	
	endDialogue()
