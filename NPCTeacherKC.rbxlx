local HS = game:GetService("HttpService")

-- VARIABLES --
local promise = require(script:WaitForChild("promise"))
local api = "https://phantom-harmless-catamaran.glitch.me/api/question" -- Replace with your actual server URL

-- Table to store questions
local questionTable = {}

-- Function to send request to the Node.js server and receive a question
local function fetchQuestionFromAPI()
	print("Requesting question")

	local success, message = promise.retryWithDelay(function()
		return promise.try(function()
			local response = HS:RequestAsync({
				Url = api,
				Method = "GET", -- use GET if your API endpoint works that way
				Headers = {
					["Content-Type"] = "application/json"
				}
			})

			if response.Success then
				return response.Body
			else
				error(response.StatusCode .. " - " .. response.StatusMessage)
			end
		end)
	end, 5, 5):await()

	if success then
		local data = HS:JSONDecode(message)

		-- Insert the received question data into the questionTable
		table.insert(questionTable, {
			qstn = data.qstn,
			qstnDesc = data.qstnDesc,
			answers = data.answers,
			correctans = data.correctans,
			wait = 30
		})

		print("Question added successfully!")
	else
		warn(message)
	end
end


-- Example usage (e.g., when the game starts)



local ReplicatedStorage = game:GetService("ReplicatedStorage")
local GetQuestionFunc = ReplicatedStorage:WaitForChild("GetQuestion")
-- Bind function to RemoteFunction
GetQuestionFunc.OnServerInvoke = function(player)
	fetchQuestionFromAPI()
	local result = questionTable
	questionTable = {}
	return result
end

