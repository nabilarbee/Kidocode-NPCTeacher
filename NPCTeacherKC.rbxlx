							<ProtectedString name="Source"><![CDATA[local PathFindingService = game:GetService("PathfindingService")
local Players = game:GetService("Players")

local npc = script.Parent.Parent
local human = npc:WaitForChild("Humanoid")
local torso = npc:WaitForChild("HumanoidRootPart")

local STOP_DISTANCE = 2 -- Distance NPC stops before reaching the player
local PATH_UPDATE_INTERVAL = 0.5 -- How often to update the path

local target = nil
local currentPath = nil
local waypoints = {}
local currentWaypointIndex = 1
local isMoving = false


local walkspeed = 24

script.Parent.Parent:FindFirstChild('Humanoid').WalkSpeed = walkspeed
-- Function to get the closest player
local function getClosestPlayer()
	local closestPlayer = nil
	local shortestDistance = 46

	for _, player in pairs(Players:GetPlayers()) do
		if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
			local playerTorso = player.Character.HumanoidRootPart
			local distance = (torso.Position - playerTorso.Position).Magnitude
			
			if distance < shortestDistance then
				shortestDistance = distance
				closestPlayer = playerTorso
			end
		end
	end

	return closestPlayer
end

-- Function to update path dynamically
local function updatePath()
	while true do
		if target then
			-- Calculate a position slightly behind the player to maintain distance
			local direction = (target.Position - torso.Position).Unit
			local stopPosition = target.Position - direction * STOP_DISTANCE

			-- Create a path with proper parameters
			local path = PathFindingService:CreatePath({
				AgentRadius = 2,
				AgentHeight = 5,
				AgentCanJump = true,
				AgentJumpHeight = 7,
				AgentMaxSlope = 45
			})

			path:ComputeAsync(torso.Position, stopPosition)

			if path.Status == Enum.PathStatus.Success then
				currentPath = path
				waypoints = path:GetWaypoints()
				currentWaypointIndex = 1
			end
		end

		wait(PATH_UPDATE_INTERVAL) -- Update the path at intervals
	end
end

-- Function to move NPC along the waypoints
local function moveNPC()
	while true do
		if target and currentPath then
			while currentWaypointIndex <= #waypoints do
				local waypoint = waypoints[currentWaypointIndex]

				-- Handle jumps
				if waypoint.Action == Enum.PathWaypointAction.Jump then
					human:ChangeState(Enum.HumanoidStateType.Jumping)
				end

				-- Move to waypoint
				human:MoveTo(waypoint.Position)
				human.MoveToFinished:Wait(0.1)

				-- Move to the next waypoint
				currentWaypointIndex += 1
			end
		end

		wait(0.1) -- Prevent overload
	end
end



--everythig here should be iside fsm vvvvvvv
-- Start path update and movement as coroutines
task.spawn(updatePath)
task.spawn(moveNPC)

-- Constantly find the closest player
while true do
	target = getClosestPlayer()
	wait(0.2)
end
]]></ProtectedString>
