							<ProtectedString name="Source"><![CDATA[local PathFindingService = game:GetService("PathfindingService")
local Players = game:GetService("Players")

local npc = script.Parent.Parent
local human = npc:WaitForChild("Humanoid")
local torso = npc:WaitForChild("HumanoidRootPart")

local NPCMovement2 = {}

local movementEnabled = true

local STOP_DISTANCE = 2
local PATH_UPDATE_INTERVAL = 0.5

local target = nil
local currentPath = nil
local waypoints = {}
local currentWaypointIndex = 1

human.WalkSpeed = 24

-- Start these on FSM startup
function NPCMovement2.start()
	movementEnabled = true
	task.spawn(NPCMovement2.updatePath)
	task.spawn(NPCMovement2.moveNPC)
	task.spawn(NPCMovement2.trackClosestPlayer)
end


function NPCMovement2.getClosestPlayer()
	return target
end

function NPCMovement2.trackClosestPlayer()
	while true do
		local closestPlayer = nil
		local shortestDistance = 46

		for _, player in pairs(Players:GetPlayers()) do
			if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
				local distance = (torso.Position - player.Character.HumanoidRootPart.Position).Magnitude
				if distance < shortestDistance then
					shortestDistance = distance
					closestPlayer = player
				end
			end
		end

		target = closestPlayer
		task.wait(0.2)
	end
end

function NPCMovement2.updatePath()
	while true do
		if movementEnabled and target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
			local direction = (target.Character.HumanoidRootPart.Position - torso.Position).Unit
			local stopPosition = target.Character.HumanoidRootPart.Position - direction * STOP_DISTANCE

			local path = PathFindingService:CreatePath({
				AgentRadius = 2,
				AgentHeight = 5,
				AgentCanJump = true,
				AgentJumpHeight = 7,
				AgentMaxSlope = 45
			})

			path:ComputeAsync(torso.Position, stopPosition)

			if path.Status == Enum.PathStatus.Success then
				currentPath = path
				waypoints = path:GetWaypoints()
				currentWaypointIndex = 1
			end
		end
		task.wait(PATH_UPDATE_INTERVAL)
	end
end


function NPCMovement2.moveNPC()
	while true do
		if movementEnabled and target and currentPath and #waypoints > 0 then
			while movementEnabled and currentWaypointIndex <= #waypoints do
				if not currentPath then break end

				local waypoint = waypoints[currentWaypointIndex]
				if waypoint.Action == Enum.PathWaypointAction.Jump then
					human:ChangeState(Enum.HumanoidStateType.Jumping)
				end

				human:MoveTo(waypoint.Position)
				human.MoveToFinished:Wait(0.1)
				currentWaypointIndex += 1
			end
		end

		task.wait(0.1)
	end
end



function NPCMovement2.clearPath()
	currentPath = nil
	waypoints = {}
	currentWaypointIndex = 1
end

function NPCMovement2.stopMoving()
	movementEnabled = false
	currentPath = nil
	waypoints = {}
	currentWaypointIndex = 1
	human:MoveTo(torso.Position + Vector3.new(0.1, 0, 0)) -- force cancel
end

function NPCMovement2.resumeMoving()
	movementEnabled = true
end


return NPCMovement2
]]></ProtectedString>
