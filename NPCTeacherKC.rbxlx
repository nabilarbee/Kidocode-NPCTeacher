							<ProtectedString name="Source"><![CDATA[local NPC = script.Parent.Parent  -- NPC MAIN PARENT REF
local Humanoid = NPC:FindFirstChild("Humanoid")
local RootPart = NPC:FindFirstChild("HumanoidRootPart")
local Players = game:GetService("Players")
local spawnPointPart = script.Parent.Parent.Parent.TeacherSpawnPoint
local originalCFrame = RootPart.CFrame
local tchrKatanaBlade = NPC.Katana.Handle.Blade
local tchrKatanaLkand = NPC.Katana.Handle.Lkand

-- Remote Events
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TriggerQuestionDialogue = ReplicatedStorage:WaitForChild("TriggerQuestionDialogue")
local TriggerKillPlayer = ReplicatedStorage:WaitForChild("TriggerKillPlayer")
local TriggerWaitQuestion = ReplicatedStorage:WaitForChild("TriggerWaitQuestion")
local TriggerEndWaitQuestion = ReplicatedStorage:WaitForChild("TriggerEndWaitQuestion")
local TriggerReturnSpawn = ReplicatedStorage:WaitForChild("TriggerReturnSpawn")

--disable range radius visual
local teacherRadiusVisual = script.Parent.Parent.Parent.TeacherSpawnPoint.TeacherRadius
teacherRadiusVisual.Transparency = 1

-- STATES DICT (TABLE)
local STATES = {
	IDLE = "Idle",
	GOPLYR = "GoToPlayer",
	ASK = "AskQuestion",
	WAITANS = "WaitForAnswer",
	CHECKANS = "CheckAnswer",
	ATKPLYR = "AttackPlayer",
	RETURN_TO_SPAWN = "ReturnToSpawn"
}

-- IMPORTANT NPC VARIABLES [INITIALISE HERE]
local npcFSM = {
	currentState = STATES.IDLE,
	targetPlayer = nil,
	spawnPosition = spawnPointPart.Position
}

-- ModuleScript References
local NPCMovement = require(script.Parent.Parent.ModuleScripts:WaitForChild("NPCMovement"))
NPCMovement.start()
local PathfindingService = game:GetService("PathfindingService")
local PATH_UPDATE_INTERVAL = 0.5

local rangeRadius = 46

local answerReceived = false
local answerWasWrong = false

TriggerKillPlayer.OnServerEvent:Connect(function(player)
	if player == npcFSM.targetPlayer then
		answerWasWrong = true
		answerReceived = true
		print("Player answered wrong, set flags")
	end
end)

local cooldown

-- When player triggers wait question, start cooldown if not already running
TriggerWaitQuestion.OnServerEvent:Connect(function(player)
	if cooldown then
		print("Cooldown already active")
		return
	end

	cooldown = task.spawn(function()
		print("Cooldown started")
		while true do
			print('waiting...')
			task.wait() -- Keeps the task alive indefinitely
		end
	end)
end)

-- When player triggers end wait question, cancel the cooldown
TriggerEndWaitQuestion.OnServerEvent:Connect(function(player)
	if cooldown then
		print("Cooldown ended manually")
		task.cancel(cooldown)
		cooldown = nil
	else
		print("No active cooldown to end")
	end
end)

TriggerReturnSpawn.OnServerEvent:Connect(function(player)
	if player == npcFSM.targetPlayer then
		answerWasWrong = false
		answerReceived = true
		print("Player answered correct, set flags")
	end
end)



-- FSM Handlers
local function enterIdle()
	
	local closestPlayer = NPCMovement.getClosestPlayer()
	if closestPlayer then
		npcFSM.targetPlayer = closestPlayer
		if npcFSM.targetPlayer.Character:FindFirstChild("Humanoid").Health ~= 0 then
			npcFSM.currentState = STATES.GOPLYR
		end
	end
end

local function enterGoToPlayer()
	if npcFSM.targetPlayer and npcFSM.targetPlayer.Character and npcFSM.targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
		local targetRoot = npcFSM.targetPlayer.Character.HumanoidRootPart
		NPCMovement.start()
		while true do
			print('chasing')
			local distance = (RootPart.Position - targetRoot.Position).Magnitude
			if distance <= 5 then
				NPCMovement.stopMoving() -- stop background path logic
				npcFSM.currentState = STATES.ASK
				print('Asking question...')
				break
			end
			local distanceHome = (targetRoot.Position - spawnPointPart.Position).Magnitude
			if distanceHome >= rangeRadius then
				NPCMovement.stopMoving() -- stop background path logic
				npcFSM.currentState = STATES.RETURN_TO_SPAWN
				print('Returning home...')
				break
			end
			wait(0.2)
		end
	else
		npcFSM.currentState = STATES.RETURN_TO_SPAWN
	end
end

local function enterAskQuestion()
	print('Triggering Dialogue Handler...')
	
	if npcFSM.targetPlayer and npcFSM.targetPlayer.Character and npcFSM.targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
		-- Make the NPC face the player
		local playerPos = npcFSM.targetPlayer.Character.HumanoidRootPart.Position
		local npcPos = RootPart.Position

		-- Create a new CFrame looking at the player, keeping NPC's current Y position to avoid tilting
		local lookAt = CFrame.new(npcPos, Vector3.new(playerPos.X, npcPos.Y, playerPos.Z))
		RootPart.CFrame = lookAt
	end
	
	
	-- Send a signal to the client
	TriggerQuestionDialogue:FireClient(npcFSM.targetPlayer)-- assuming you have a reference to 'player'
	npcFSM.currentState = STATES.WAITANS
end




local function enterWaitForAnswer()
	answerReceived = false
	answerWasWrong = false
	print("Waiting for player's answer...")

	TriggerWaitQuestion:FireClient(npcFSM.targetPlayer) -- Start client timer

	-- Loop until answer is received (either correct or wrong), or forcibly timeout here if needed (server backup failsafe)

	while not answerReceived do
		task.wait()
	end

	TriggerEndWaitQuestion:FireClient(npcFSM.targetPlayer) -- Always force the client to stop their timer

	if answerReceived then
		if answerWasWrong then
			npcFSM.currentState = STATES.ATKPLYR
		else
			npcFSM.currentState = STATES.RETURN_TO_SPAWN
		end
	else
		print("Player didn't answer, assume wrong and attack")
		npcFSM.currentState = STATES.ATKPLYR
	end
end


local function enterCheckAnswer()
	
end

local function enterAttackPlayer()
	print("Attacking player...")
	
	tchrKatanaBlade.Transparency = 0
	tchrKatanaLkand.Transparency = 0

	local attackAnim = NPC:FindFirstChild("slash1")
	local attackAnim2 = NPC:FindFirstChild("slash2")
	local attackAnim3 = NPC:FindFirstChild("slash3")
	local attackAnim4 = NPC:FindFirstChild("slash4")
	local attackAnim5 = NPC:FindFirstChild("slash5")
	
	if not attackAnim then
		warn("AttackAnimation not found!")
		return
	end
	local targetHum = npcFSM.targetPlayer.Character:FindFirstChild("Humanoid")
	targetHum.WalkSpeed = 0
	
	-- Load and play animation
	local animator = Humanoid:FindFirstChildOfClass("Animator")
	if not animator then
		animator = Instance.new("Animator")
		animator.Parent = Humanoid
	end
	local swingSound1 = script.Parent.Parent.Audio.swingSound1
	local swingSound2 = script.Parent.Parent.Audio.swingSound2
	local swingSound3 = script.Parent.Parent.Audio.swingSound3
	local swingSound4 = script.Parent.Parent.Audio.swingSound4
	local swingSound5 = script.Parent.Parent.Audio.swingSound5
	
	local animTrack = animator:LoadAnimation(attackAnim)
	animTrack:Play()
	
	task.wait(0.5)
	swingSound1:Play()
	animTrack = animator:LoadAnimation(attackAnim2)
	animTrack:Play()
	
	task.wait(0.5)
	swingSound2:Play()
	animTrack = animator:LoadAnimation(attackAnim3)
	animTrack:Play()
	
	task.wait(0.5)
	swingSound4:Play()
	animTrack = animator:LoadAnimation(attackAnim4)
	animTrack:Play()
	
	task.wait(0.5)
	swingSound5:Play()
	animTrack = animator:LoadAnimation(attackAnim5)
	animTrack:Play()
	
	task.wait(0.5)
	swingSound3:Play()
	task.wait(0.5)

	-- Set player HP to 0 (simulate attack hit)
	if npcFSM.targetPlayer and npcFSM.targetPlayer.Character then
		
		if targetHum then
			targetHum.Health = 0
		end
	end
	tchrKatanaBlade.Transparency = 1
	tchrKatanaLkand.Transparency = 1
	-- Return to spawn or idle after attacking
	npcFSM.currentState = STATES.RETURN_TO_SPAWN
end


local function enterReturnToSpawn()
	-- Create the path
	local path = PathfindingService:CreatePath({
		AgentRadius = 2,
		AgentHeight = 5,
		AgentCanJump = true,
		AgentJumpHeight = 10,
		AgentMaxSlope = 45,
	})

	-- Compute path to the part
	path:ComputeAsync(RootPart.Position, npcFSM.spawnPosition)

	-- Visualize or walk the path
	if path.Status == Enum.PathStatus.Success then
		local waypoints = path:GetWaypoints()

		for _, waypoint in ipairs(waypoints) do
			Humanoid:MoveTo(waypoint.Position)
			Humanoid.MoveToFinished:Wait()
		end
		-- Reset only rotation, keep current position
		RootPart.CFrame = CFrame.new(RootPart.Position) * CFrame.Angles(0, originalCFrame.Rotation.Y, 0)
		npcFSM.currentState = STATES.IDLE
	else
		warn("Path not found")
	end
	-- goto find waypoint go to spawn
	-- change state to idle/search
end

-- LOOP HANDLER (Acts as Void Update)
while true do
	if npcFSM.currentState == STATES.IDLE then
		enterIdle()
	elseif npcFSM.currentState == STATES.GOPLYR then
		enterGoToPlayer()
	elseif npcFSM.currentState == STATES.ASK then
		enterAskQuestion()
	elseif npcFSM.currentState == STATES.WAITANS then
		enterWaitForAnswer()
	elseif npcFSM.currentState == STATES.CHECKANS then
		enterCheckAnswer()
	elseif npcFSM.currentState == STATES.ATKPLYR then
		enterAttackPlayer()
	elseif npcFSM.currentState == STATES.RETURN_TO_SPAWN then
		enterReturnToSpawn()
	end
	task.wait(0.5)  -- SO LOOP DOESNT GO TOO FAST (changes states at fastest every 0.5 seconds)
end
